			<input type="checkbox" onclick="filtrSettings();" id="digit" name="checkbox" nochecked>
			<lable>Numeric digits</lable><br>

			<input type="checkbox" onclick="filtrSettings();" id="upperCase" name="checkbox" nochecked>
			<lable>Upper Case</lable><br>

			<input type="checkbox" onclick="filtrSettings();" id="lowerCase" name="checkbox"  nochecked>
